<body>
		<span class="pigeonLogoLink"><a href="landing"><i class="fas fa-dove"></i> Pigeon Project</a></span>
	<!-- Registration Form -->
	<div class="registrationFormContent">
		<center><img class="registerLandImg animated bounceIn" src="assets/images/man-in-binoculars.jpg"></center>
		<div class="animated fadeIn ">
		<form class="ui form">
			<p class="ui horizontal divider header regTitle">Find Others</p>
  <div class="field">
    <label>Name</label>
    <div class="two fields">
      <div class="field">
        <input type="text" name="firstName" placeholder="First Name">
      </div>
      <div class="field">
        <input type="text" name="lastName" placeholder="Last Name">
      </div>
    </div>
  </div>
  <div class="field">
    <label>Username</label>
    <div class="fields">
      <div class="sixteen wide field">
        <input type="text" name="username" placeholder="Johnny Appleseed">
      </div>
    </div>
  </div>
	<div class="field">
    <label>Email</label>
    <div class="fields">
      <div class="sixteen wide field">
        <input type="email" name="email" placeholder="name@example.com">
      </div>
    </div>
  </div>
	<div class="field">
		<label>Password</label>
		<div class="fields">
			<div class="sixteen wide field">
				<input type="password" name="password" placeholder="Create Password">
			</div>
		</div>
		</div>
		<center><span id="termsText">By creating an account, you agree to our <a href="">Terms & Conditions</a></span></center>
	<button class="ui submit button registerBtn">Register!</button>
	<center><small class="accountSwitchText">Already have an account? <a href="login.hbs">Login</small></center>
		</div>
</body>
